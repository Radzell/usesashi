{
  "short_name": "sashi-extension",
  "name": "Sashi Extension",
  "version": "0.3.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["tailwindcss-output.css"],
      "js": ["index.tsx"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["storage", "scripting", "tabs"],
  "icons": {}
}
