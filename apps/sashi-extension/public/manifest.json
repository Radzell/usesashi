{
  "short_name": "sashi-extension",
  "name": "Sashi Extension",
  "version": "0.3.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["dist/index.d8831b8e.css"],
      "js": ["dist/index.1d7c845c.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["storage", "scripting", "tabs"],
  "icons": {}
}
